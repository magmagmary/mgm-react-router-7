	},
	"dependencies": {
		"@react-router/node": "7.1.3",
		"@react-router/serve": "7.1.3",
		"isbot": "5.1.25",
		"react": "19.1.0",
		"react-dom": "19.1.0",
		"react-router": "7.1.3",
		"zod": "^3.25.13"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@react-router/dev": "7.1.3",
		"@tailwindcss/vite": "4.0.16",
		"@types/gh-pages": "6.1.0",
		"@types/node": "22.15.21",
		"@types/react": "19.1.5",
		"@types/react-dom": "19.1.5",
		"cross-env": "7.0.3",
		"gh-pages": "6.3.0",
		"tailwindcss": "4.0.16",
		"typescript": "5.8.2",
		"vite": "6.2.3",
		"vite-tsconfig-paths": "5.1.4",
		"vitest": "^3.1.3"
